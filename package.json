    "@storybook/react-native": "4.1.16",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.12",
    "@types/react-native": "^0.57.42",
    "@types/react-test-renderer": "^16.8.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.7.1",
    "babel-runtime": "^6.26.0",
    "jest": "^24.7.1",
